@page "/bio"

<h1 style="color:black;">Bio</h1>

<p>Output: @OutputString </p>
@code
{

class Biography

    {

        string name;
        string age;
        string gender;
        string height;
        string weight;
        

        public void doProcessing(){
        Console.WriteLine(name + "\n");
        Console.WriteLine(age + "\n");
        Console.WriteLine(gender + "\n");
        Console.WriteLine(height +"\n");
        Console.WriteLine(weight);
        }




        static void Main()
        {
            Biography Aaron = new Biography();
            Aaron.name = "Aaron Jones";
            Aaron.age = "21";
            Aaron.gender = "Male";
            Aaron.height = "177 cm";
            Aaron.weight = "88 kg";

            string OutputString = Aaron.doProcessing();
        }

    }
}